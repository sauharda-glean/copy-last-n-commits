{
  "name": "Copy Last N Commits",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {},
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Shift+Ctrl+C",
        "mac": "Command+Shift+Z"
      }
    }
  }
}